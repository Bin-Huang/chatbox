        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-import-resolver-webpack": "^0.13.2",
        "eslint-plugin-compat": "^4.1.4",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-jsx-a11y": "^6.7.1",

        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths-webpack-plugin": "^4.0.1",
        "typescript": "^4.9.5",
        "url-loader": "^4.1.1",
        "webpack": "^5.85.0",

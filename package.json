        "electron-store": "^8.1.0",
        "electron-updater": "^5.3.0",
        "eventsource-parser": "^1.0.0",
        "form-data": "^4.0.0",
        "fs-extra": "^11.1.1",
        "highlight.js": "^11.7.0",
        "i18next": "^22.4.13",
        "jotai": "^2.1.0",
        "jotai-optics": "^0.3.0",
        "js-tiktoken": "^1.0.7",
        "lodash": "^4.17.21",
        "lucide-react": "^0.285.0",
        "ofetch": "^1.0.1",
        "optics-ts": "^2.4.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^12.2.0",
        "react-markdown": "^9.0.0",
        "react-router-dom": "^6.11.2",
        "react-syntax-highlighter": "^15.5.0",
        "react-zoom-pan-pinch": "^3.4.4",
        "rehype-katex": "^7.0.0",
        "remark-breaks": "^4.0.0",
        "remark-gfm": "^4.0.0",
        "remark-math": "^6.0.0",
        "sanitize-filename": "^1.6.3",
